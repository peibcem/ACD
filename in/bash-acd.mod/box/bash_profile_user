# Historial de hasta 5000 eventos en esta sesi√≥n y en el fichero \
# (con todas las sesiones)
HISTSIZE=5000
HISTFILESIZE=5000

# Colors:
export PS1='${debian_chroot:+($debian_chroot)}\[\033[0;31m\][\t]\[\033[01;32m\]\u@\h\[\033[00m\]:\[\033[01;34m\]\w\[\033[01;37m\]$\[\033[00m\] '

if [ -f ~/.bashrc ]; then
	source ~/.bashrc
fi
